//C language

//hdoj-2006

#include <stdio.h>
int main()
{
int n,i,t,sum;
while(scanf("%d",&n)!=EOF)
{
    sum=1;
    for(i=0;i<n;i++)
    {
        scanf("%d",&t);
        if(t%2==1)
            sum*=t;
    }
    printf("%d\n",sum);
}
    return 0;
}

//hdoj-2007

#include <stdio.h>
int main()
{
int m,n,x,y,i;
while(scanf("%d%d",&n,&m)!=EOF)
{
    x=y=0;
    if(m>n){i=m,m=n,n=i;}
    for(m;m<=n;m++)
    {
        if(m%2==1)
            y+=m*m*m;
        else
            x+=m*m;
    }
        printf("%d %d\n",x,y);
    }
    return 0;
}

//hjoj 2008 
!!attention float is not suitable.and x=x=1 doesn't work(still need learning)

#include <stdio.h>
int main()
{
int n,i,x,y,z;
double a;
while(scanf("%d",&n)!=EOF&&(n!=0))
{
    x=y=z=0;
    for(i=0;i<n;i++)
    {
        scanf("%lf",&a);
        if(a<0)
            x++;
        else if(a==0)
            y++;
        else if(a>0)
            z++;
    }
        printf("%d %d %d\n",x,y,z);
    }
    return 0;
}

//hdoj 2009
!!oh my god! data structure is so important.....

#include <stdio.h>
#include<math.h>

int main()
{
int m;
double n,sum=0.00;
//n=x=sum;
while(scanf("%lf%d",&n,&m)!=EOF)
{

    for(m>1;m--;n=sqrt(n))
    {

        sum+=n;

    }
        printf("%.2lf\n",sum);
    }
    return 0;
}

